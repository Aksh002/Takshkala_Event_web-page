{
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/no-unknown-property": ["error", { "ignore": ["jsx", "global"] }]
      },
    "extends": ["next", "prettier"]
}
